"qualityCheck": {
	"_id": 		{ "type": "string" },
	"site":		{ "type": "string" },
	"datetime":	{ "type": "date" },
	"fileType": 	{ "type": "string" },
	"systems": 	{
		"properties": 	{
			"systemName": 	{ "type": "string" },
			"metrics": 	{
				"type": 	"object",
				"properties": 	{
					"codes": 	{
						"properties": 	{
							"obsType": 	{ "type": "str" },
							"expObs": 	{ "type": "integer" },
							"havObs":	{ "type": "integer" },
							"multipath":	{ "type": "float" }
						}
					},
					"phases": 	{
						"properties": 	{
							"obsType":      { "type": "str" },
                                	        	"expObs":       { "type": "integer" },
	                                	        "havObs":       { "type": "integer" },
							"cycleSlips":	{ "type": "integer" }
						}
					}
				}
			}
		}
	}
}

# e.g.
# _id must represent results for specific site, time, and type - so newer QC metrics always overwrite

{
	"_id": 		"ALIC20162510000DAILY",
	"site":		"ALIC",
	"datetime": 	"20162510000",
	"fileType": 	"daily",
	"systems": 	[
		{
			"systemName": 	"GPS",
			"metrics": 	{
				"codes": 	[
					{
						"obsType": 	"C1C",
						"expObs": 	9566,
						"havObs":	7066,
						"multipath":	15.754
					}, { ... }, { ... }
				],
				"phases": 	[
					{
						"obsType": 	"L1C",
						"expObs": 	9566,
						"havObs":	7066,
						"cycleSlips": 	21
					}, { ... }, { ... }
				]
			}
		}, { ... }, { ... }
	]
}




# Alternative
# Should "codes" and "phases" be different objects as above, or just null value multipath or cyclesplips depending?

"observations": {
	"_id": 		{ "type": "string" },
	"fileType":	{ "type": "string" },
	"site":		{ "type": "string" },
	"datetime":	{ "type": "date" },
	"system":	{ "type": "string" },
	"obsType":	{ "type": "string" },
	"expObs": 	{ "type": "integer" },
	"havObs":	{ "type": "integer" },
	"multipath": 	{ "type": "float" }
	"cycleSlips": 	{ "type": "integer" }
}

# e.g.

{
	"_id":		"ALIC20162510000DAILYGPSC1C",
	"fileType":	"daily",
	"site":		"ALIC",
	"datetime":	"20162510000",
	"system":	"GPS",
	"obsType":	"C1C",
	"expObs":	9566,
	"havObs":	7066,
	"multipath":	15.754
}

{
	"_id":		"ALIC20162510000DAILYGPSL1C",
	"fileType":	"daily",
	"site":		"ALIC",
	"datetime":	"20162510000",
	"system":	"GPS",
	"obsType":	"L1C",
	"expObs":	9566,
	"havObs":	6834,
	"cycleSlips":	21
}


# Alt alt

"qualityCheck": {
        "_id":          { "type": "string" },
        "site":         { "type": "string" },
        "datetime":     { "type": "date" },
        "fileType":     { "type": "string" },
        "metrics":      {
                "properties":   {
                        "systemName":   { "type": "string" },
			"obsType":      { "type": "str" },
			"expObs":       { "type": "integer" },
			"havObs":       { "type": "integer" },
			"multipath":    { "type": "float" }
			"cycleSlips":   { "type": "integer" }
		}
        }
}

# e.g.

{
        "_id":          "ALIC20162510000DAILY",
        "site":         "ALIC",
        "datetime":     "20162510000",
        "fileType":     "daily",
        "metrics":      [
                {
                        "systemName":   "GPS",
			"obsType":      "C1C",
			"expObs":       9566,
			"havObs":       7066,
			"multipath":    15.754
		}, {
			"systemName": 	"GPS",
			"obsType":      "L1C",
			"expObs":       9566,
			"havObs":       7066,
			"cycleSlips":   21
		}, { ... }, { ... }
	]
}


